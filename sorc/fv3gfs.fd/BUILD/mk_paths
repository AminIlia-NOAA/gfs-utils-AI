    shared/ \
    atmos_cubed_sphere/driver/coupled/ \
    atmos_cubed_sphere/tools/ \
    atmos_cubed_sphere/model/ \
    atmos_cubed_sphere/model_nh/ \
    atmos_cubed_sphere/lin_cloud_microphys/ \
    gfs_physics/driver \
    gfs_physics/sfc_cycling/ \
    gfs_physics/ozinterp/ \
    atmos_drivers/coupled/ \
    simple_coupler/coupler_main.F90
